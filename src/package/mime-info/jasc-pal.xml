<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
	<mime-type type="application/x-jasc-palette">
		<sub-class-of type="text/plain"/>
		<comment>JASC Palette File</comment>
		<glob pattern="*.pal"/>
		<magic>
			<match value="JASC-PAL" type="string" offset="0"/>
		</magic>
	</mime-type>
</mime-info>
